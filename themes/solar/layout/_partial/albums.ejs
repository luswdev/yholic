<div class="albums">
    <h1><%= title %></h1>
    <div class="row row-cols-1 row-cols-md-2 row-cols-lg-3 g-4">
        <% site.pages.forEach(page => {
            if (page.type !== type) return;
        %>
            <%- partial('_partial/album', { page: page }) %>
        <% }); %>
    </div>
</div>